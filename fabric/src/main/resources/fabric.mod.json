{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${display_name}",
  "description": "${description}",
  "mixins": [
    "figurasvc-common.mixins.json"
  ],
  "authors": [
    "${authors}"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "LGPL-2.1",
  "icon": "assets/figurasvc/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.knownsh.figurasvc.fabric.FiguraSVCFabric"
    ],
    "figura_api": [
      "net.knownsh.figurasvc.FiguraSVC"
    ],
    "figura_event": [
      "net.knownsh.figurasvc.legacy.VoiceChatEventLegacy"
    ],
    "voicechat": [
      "net.knownsh.figurasvc.svc.SVCPlugin"
    ]
  },
  "depends": {
    "fabric": "*",
    "minecraft": ">=1.20",
    "figura": ">=${figura_version}"
  }
}